#include <bits/stdc++.h>
#define int long long
#define nl " \n"

using namespace std;

const int mxn = 2e5;
int a[mxn];

void solve() {
     int n; cin >> n;
     int s = 0;
     for (int i = 0; i < n; i++)
         cin >> a[i],
         s += a[i];

    int q; cin >> q;
    while (q--) {
        int x; cin >> x;
        cout << (s + q - 1) / q << nl;
}

signed main() {
    cin.tie(0)->sync_with_stdio(false);

    int t = 1; 
    while (t--)
        solve();
}
